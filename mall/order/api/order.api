type (
	OrderReq {
		Id string `path:"id"`
	}

	OrderReply {
		Id     string `json:"id"`
		Name   string `json:"name"`
		Age    int64  `json:"age"`
		Gender string `json:"gender"`
	}
)

@server(
	group: order
	middleware: AuthMiddleware
)
service order {
	@handler getOrder
	get /api/order/get/:id (OrderReq) returns (OrderReply)
}